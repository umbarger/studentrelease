  <div class="control-group">
    <%= f.label :active, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :active, :class => 'form-control' %>
    </div>
    <%= error_span(@vehicle[:active]) %>
  </div>


  <%= f.submit nil, :class => 'btn btn-primary' %>

<% end %>
