<%= form_for [@family, @vehicle], :html => { :class => "form-horizontal vehicle" } do |f| %>

    <% if @vehicle.errors.any? %>
    <div id="error_expl" class="panel panel-danger">
      <div class="panel-heading">
        <h3 class="panel-title"><%= pluralize(@vehicle.errors.count, "error") %> prohibited this vehicle from being saved:</h3>
      </div>
      <div class="panel-body">
        <ul>
        <% @vehicle.errors.full_messages.each do |msg| %>
          <li><%= msg %></li>
        <% end %>
        </ul>
      </div>S
    </div>
  <% end %>

  <div class="control-group">
    <%= f.label :year, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :year, :class => 'form-control' %>
    </div>
    <%= error_span(@vehicle[:year]) %>
  </div>
  <div class="control-group">
    <%= f.label :make, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :make, :class => 'form-control' %>
    </div>
    <%= error_span(@vehicle[:make]) %>
  </div>
  <div class="control-group">
    <%= f.label :model, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :model, :class => 'form-control' %>
    </div>
    <%= error_span(@vehicle[:model]) %>
  </div>
  <div class="control-group">
    <%= f.label :color, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :color, :class => 'form-control' %>
    </div>
    <%= error_span(@vehicle[:color]) %>
  </div>
  <div class="control-group">
    <%= f.label :license, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :license, :class => 'form-control' %>
    </div>
    <%= error_span(@vehicle[:license]) %>
  </div>
  <div class="control-group">
    <%= f.label :state, :class => 'control-label' %>
    <div class="controls">
